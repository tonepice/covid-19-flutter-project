    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "dev",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",   
            "args": [
                "-t",
                "lib/main_dev.dart",
                "--flavor",
                "dev",
            ] 
        },
        {
            "name": "prod",
            "request": "launch",
            "type": "dart",
            "program": "lib/main_prod.dart",   
            "args": [
                "-t",
                "lib/main_prod.dart",
                "--flavor",
                "prod",
            ] 
        },
        {
            "name": "covid-19-flutter",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "covid-19-flutter (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "covid-19-flutter (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}